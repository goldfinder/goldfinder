if not (blender == nil) then
local bv = Instance.new("BodyVelocity")
bv.P = 500
bv.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
bv.Velocity = blender.CFrame.lookVector*-FlingAmount
bv.Parent = blender
wait(.05)
bv:Destroy()
wait(.2)
end
end
script.Parent.Touched:connect(hit)
